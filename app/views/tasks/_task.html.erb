<div id="<%= dom_id task %>">
  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Due date:</strong>
    <%= task.due_date %>
  </p>

  <p>
    <strong>Due time:</strong>
    <%= task.due_time %>
  </p>

  <p>
    <strong>Completion:</strong>
    <%= task.completion %>
  </p>

  <p>
    <strong>Ping frequency:</strong>
    <%= task.ping_frequency %>
  </p>

